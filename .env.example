# Copy this file to .env and fill in your values

# LLM API Keys (at least one required)
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Default LLM Configuration
DEFAULT_MODEL=anthropic/claude-3.5-sonnet
DEFAULT_TEMPERATURE=0.7
DEFAULT_MAX_TOKENS=4096

# Agent-Specific Models (optional overrides)
ARCHITECT_MODEL=anthropic/claude-3.5-sonnet
PRODUCT_MODEL=anthropic/claude-3.5-sonnet
IMPLEMENTATION_MODEL=anthropic/claude-3.5-sonnet
REVIEWER_MODEL=anthropic/claude-3.5-sonnet
BUILD_TEST_MODEL=anthropic/claude-3.5-sonnet
INTEGRATOR_MODEL=anthropic/claude-3.5-sonnet
INFRA_MODEL=anthropic/claude-3.5-sonnet

# System Configuration
LOG_LEVEL=INFO
MACDS_DATA_DIR=.macds

# Memory Configuration
MEMORY_DECAY_ENABLED=true
MEMORY_CLEANUP_THRESHOLD=0.1

# Execution Configuration
MAX_WORKFLOW_RETRIES=3
EXECUTION_TIMEOUT_SECONDS=300
